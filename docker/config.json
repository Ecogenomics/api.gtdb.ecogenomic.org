{
  "listeners": {
    "*:80": {
      "pass": "applications/fastapi"
    }
  },
  "applications": {
    "fastapi": {
      "type": "python 3.13",
      "processes": 4,
      "path": "/api",
      "home": "/usr/local",
      "module": "main",
      "callable": "app"
    }
  },
  "access_log": "/dev/stdout"
}
